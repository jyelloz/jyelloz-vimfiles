{
  "rust.cfg_test": true,
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
      "rootPatterns": [
        ".ccls",
        "compile_commands.json",
        "build/compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "initializationOptions": {
         "cache": {
           "directory": ".ccls-cache"
         }
       }
    }
  }
}
